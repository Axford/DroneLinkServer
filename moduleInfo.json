            },
            {
                "address": 14,
                "type": "f",
                "numValues": 1,
                "name": "publishRate",
                "description": "Rate of messages published per second"
            },
            {
                "address": 15,
                "type": "u32",
                "numValues": 1,
                "name": "choked",
