            }
        ],
        "sub": [
            {
                "address": 16,
                "addrAddress": 17,
                "type": "f",
                "numValues": 2,
                "name": "location",
                "description": "Current location from GPS"
            }
        ],
        "filename": [
            "../DroneNode/src/droneModules/HMC5883LModule.h"
        ]
